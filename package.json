    "express": "^4.18.2",
    "joi": "^17.4.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.12.5",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@baemingo/tscpaths-async": "^0.0.15",
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^4.3.10",
    "@types/express": "^4.17.21",
    "@types/hapi__joi": "^17.1.6",
    "@types/mocha": "^10.0.4",
    "@types/multer": "^1.4.10",
    "@types/supertest": "^2.0.16",
