    "@typescript-eslint/parser": "^6.10.0",
    "chai": "^4.3.10",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "shx": "^0.3.3",
    "supertest": "^6.3.3",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^5.2.2"
  }
