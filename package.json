{
  "name": "movies-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "shx rm -rf lib/ && tsc && npm run post:compile",
    "post:compile": "tscpaths -p tsconfig.json -s ./src -o ./lib > nul 2>&1",
    "start": "npm run compile && node lib/",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@baemingo/tscpaths-async": "^0.0.15",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "shx": "^0.3.3",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^5.2.2"
  }
}
